<h1>Admins::Members#index</h1>
<p>Find me in app/views/admins/members/index.html.erb</p>
<table>
    <tr>
        <th>会員ID</th>
        <th>氏名</th>
        <th>メールアドレス</th>
        <th>ステータス</th>
    </tr>
    <% @members.each do |member| %>
    <tr>
        <td>
            <%= member.id%>
        </td>
        <td>
            <%= link_to admins_member_path(member.id) do %>
            <%= member.last_name %>
            <%= member.first_name %>
            <% end %>
        </td>
        <td>
            <%= member.email %>
        </td>
        <td>
            <% if member.withdrawal_status == 1 %>
            <p>有効</p>
            <% else %>
            <p>退会済</p>
            <% end %>
        </td>
    </tr>
    <% end %>
</table>